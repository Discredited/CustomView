<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.project.june.customview">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".CustomApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.progress.MochaProgressActivity" />
        <activity android:name=".ui.level.ProgressActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.first.FirstCustomActivity" />
        <activity android:name=".ui.second.SecondCustomActivity" />
        <activity android:name=".ui.tantag.TanTagViewActivity" />
        <activity android:name=".ui.color.ColorActivity" />
        <activity android:name=".ui.magnifier.MagnifierActivity" />
        <activity android:name=".ui.answer.AnswerRushActivity" />
        <activity android:name=".ui.video.VideoActivity" />
        <activity android:name=".ui.video.VideoListActivity" />
    </application>

</manifest>